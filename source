#include <iostream>
#include <clocale>
#include <cmath>
#include <ctime>
using namespace std;
void shell(int* a , int size)
{
	int  j, gap, h;
	gap = size / 2;
	while (gap>0)
	{
		for (int i = 0; i < (size - gap); i++)
		{
			j = i;
			while ((j >= 0) && (a[j] > a[j + gap]))
			{
				h = a[j];
				a[j] = a[j + gap];
				a[j + gap] = h;
				j--;
			}
		}
		gap /= 2;
	}	
}
void heapify(int arr[], int n, int i)
{
    int largest = i;
    int left = 2 * i + 1;
    int right = 2 * i + 2;
    if (left < n && arr[left] > arr[largest])
        largest = left;
    if (right < n && arr[right] > arr[largest])
        largest = right;
    if (largest != i)
    {
        swap(arr[i], arr[largest]);
        heapify(arr, n, largest);
    }
}
void heapSort(int arr[], int n)
{
    for (int i = n / 2 - 1; i >= 0; i--)
        heapify(arr, n, i);
    for (int i = n - 1; i >= 0; i--)
    {
        swap(arr[0], arr[i]);
        heapify(arr, i, 0);
    }
}
int main()
{
int n;
	cin >> n;
	int* a = new int[n];
	srand(time(0));
	for (int i = 0; i < n; i++)
	{
		a[i] = rand() % n + 1;
	}
delete[] a;
return 0;
}
